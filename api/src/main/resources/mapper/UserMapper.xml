<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lea.api.mapper.UserMapper">


    <select id="findAll" resultType="com.lea.api.entity.User">
        select id, `name`, age, email, create_time createTime
        from user
    </select>

</mapper>